{
  "name": "@sentinelseed/solana-agent-kit",
  "version": "1.0.0",
  "description": "Sentinel safety validation plugin for Solana Agent Kit - THSP protocol for AI agent transaction safety",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "solana-agent-kit",
    "ai-safety",
    "sentinel",
    "thsp",
    "transaction-validation",
    "ai-agent",
    "blockchain-safety",
    "web3",
    "defi-safety"
  ],
  "author": "Sentinel Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sentinel-seed/sentinel.git",
    "directory": "integrations/solana-agent-kit/typescript"
  },
  "bugs": {
    "url": "https://github.com/sentinel-seed/sentinel/issues"
  },
  "homepage": "https://sentinelseed.dev",
  "peerDependencies": {
    "@solana/web3.js": "^1.98.0",
    "solana-agent-kit": "^2.0.0"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.98.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "solana-agent-kit": "^2.0.7",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
