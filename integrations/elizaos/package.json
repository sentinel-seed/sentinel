{
  "name": "@sentinelseed/elizaos-plugin",
  "version": "0.1.0",
  "description": "Sentinel AI safety plugin for ElizaOS - THSP protocol validation for autonomous agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "elizaos",
    "eliza",
    "ai-safety",
    "alignment",
    "sentinel",
    "thsp",
    "autonomous-agents",
    "ai-agents",
    "plugin",
    "web3",
    "crypto"
  ],
  "author": "Sentinel Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sentinel-movement/sentinel.git"
  },
  "homepage": "https://sentinelseed.dev",
  "peerDependencies": {
    "@elizaos/core": ">=0.1.0"
  },
  "dependencies": {
    "sentinelseed": "^0.2.0"
  },
  "devDependencies": {
    "@elizaos/core": "^1.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
